<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ListServlet</servlet-name>
        <servlet-class>controller.ListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletLogin</servlet-name>
        <servlet-class>controller.ServletLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletSignUp</servlet-name>
        <servlet-class>controller.ServletSignUp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletListProduct</servlet-name>
        <servlet-class>controller.ServletListProduct</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletProductDetail</servlet-name>
        <servlet-class>controller.ServletProductDetail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletAddCart</servlet-name>
        <servlet-class>controller.ServletAddCart</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddProduct</servlet-name>
        <servlet-class>controller.AddProduct</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletMyOrder</servlet-name>
        <servlet-class>controller.ServletMyOrder</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteItem</servlet-name>
        <servlet-class>controller.DeleteItem</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletSave</servlet-name>
        <servlet-class>controller.ServletSave</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BuyProduct</servlet-name>
        <servlet-class>controller.BuyProduct</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletProcess</servlet-name>
        <servlet-class>controller.ServletProcess</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletSearch</servlet-name>
        <servlet-class>controller.ServletSearch</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteProduct</servlet-name>
        <servlet-class>controller.DeleteProduct</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListServlet</servlet-name>
        <url-pattern>/list</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletLogin</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletSignUp</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletListProduct</servlet-name>
        <url-pattern>/listPro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletProductDetail</servlet-name>
        <url-pattern>/prodetail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletAddCart</servlet-name>
        <url-pattern>/cart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddProduct</servlet-name>
        <url-pattern>/addpro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletMyOrder</servlet-name>
        <url-pattern>/myorder</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteItem</servlet-name>
        <url-pattern>/delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletSave</servlet-name>
        <url-pattern>/save</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BuyProduct</servlet-name>
        <url-pattern>/buy</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletProcess</servlet-name>
        <url-pattern>/process</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletSearch</servlet-name>
        <url-pattern>/search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteProduct</servlet-name>
        <url-pattern>/deletePro</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
